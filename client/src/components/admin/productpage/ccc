    }


      if (this.state.dependentField[4] && this.state.dependentField[4].subField === 'Yes') {
      sixthArg = this.state.listVariants[4].list[0].value;
      // sixthArg = null;
      // seventhArg = null;
    } else if (this.state.dependentField[4] && this.state.dependentField[4].subField === 'No') {
      if (sixthArg) {
        seventhArg = this.state.dependentField[4].mappingLabel
      }
    }